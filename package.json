{
  "name": "@anopszetex/graphql-api-ts",
  "version": "1.0.0",
  "description": "A simple project using JWT Authentication with Node and Graphql",
  "main": "index.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean:build": "rm -rf dist/",
    "start": "NODE_ENV=production node dist/index.js",
    "dev": "tsnd --respawn -- src/index.ts",
    "lint": "eslint src/ --ext .ts",
    "lint:ci": "eslint -f json -o coverage/eslint.json src/ --ext .ts",
    "format": "prettier src/ -w -u",
    "format:check": "prettier src/ -c -u",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "migrate:latest": "npx knex migrate:latest",
    "migrate:rollback": "npx knex migrate:rollback",
    "seed:run": "npx knex seed:run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-typescript": "^7.18.6",
    "@faker-js/faker": "^7.5.0",
    "@types/jest": "^29.1.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.8.1",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "apollo-server-express": "^3.10.3",
    "babel-jest": "^29.1.2",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-security-node": "^1.1.1",
    "jest": "^29.1.2",
    "ntl": "^5.1.0",
    "pino-pretty": "^9.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@graphql-tools/load-files": "^6.6.1",
    "@graphql-tools/merge": "^8.3.6",
    "apollo-server": "^3.10.2",
    "argon2": "^0.29.1",
    "dotenv": "^16.0.3",
    "graphql": "^16.6.0",
    "hyperid": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.3.0",
    "lodash": "^4.17.21",
    "mongoose": "^6.6.5",
    "pg": "^8.8.0",
    "pg-query-stream": "^4.2.4",
    "pino": "^8.6.1"
  }
}
